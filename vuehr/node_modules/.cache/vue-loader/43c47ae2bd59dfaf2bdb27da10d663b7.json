tonText: '取消',\r\n          type: 'warning'\r\n        }).then(() => {\r\n          this.getRequest(\"/logout\");\r\n          window.sessionStorage.removeItem(\"user\")\r\n          this.$store.commit('initRoutes', []);\r\n          this.$router.replace(\"/\")\r\n        }).catch(() => {\r\n          this.$message({\r\n            type: 'info',\r\n            message: '已取消删除'\r\n          });\r\n        });\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.homeRouterView {\r\n  margin-top: 10px;\r\n}\r\n\r\n.homeWelcome {\r\n  text-align: center;\r\n  font-size: 30px;\r\n  font-family: 微软雅黑;\r\n  color: #409eff;\r\n  padding-top: 50px;\r\n}\r\n\r\n.homeHeader {\r\n  background-color: #409eff;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 0px 15px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.homeHeader .title {\r\n  font-size: 30px;\r\n  font-family: 微软雅黑;\r\n  color: #ffffff;\r\n}\r\n\r\n.homeHeader .userInfo {\r\n  cursor: pointer;\r\n}\r\n\r\n.el-dropdown-link img {\r\n  width: 48px;\r\n  height: 48px;\r\n  border-radius: 24px;\r\n  margin-left: 8px;\r\n}\r\n\r\n.el-dropdown-link {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n</style>"]}]}